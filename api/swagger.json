{
    "schemes": [
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "DORY (stands for Domain Operator - Recover by Yourself) is a tool who enable user to unlock their AD account, and change/reinitialize password. This API is NOT intented to be RESTful !",
        "title": "Dory-API",
        "contact": {
            "name": "Cloud dev team",
            "url": "https://github.com/be-ys-cloud"
        },
        "license": {
            "name": "AGPL",
            "url": "https://www.gnu.org/licenses/agpl-3.0.fr.html"
        },
        "version": "1.0.0"
    },
    "basePath": "/",
    "paths": {
        "/ask_reinitialization": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Ask server to send email with a link to reset an account password.",
                "tags": [
                    "reinitialization"
                ],
                "summary": "Ask server to send email with a link to reset an account password.",
                "parameters": [
                    {
                        "description": "User (only username is required)",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/structures.UserAsk"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK - Check your mailbox"
                    },
                    "400": {
                        "description": "Missing username in payload"
                    },
                    "500": {
                        "description": "An error occured."
                    }
                }
            }
        },
        "/ask_unlock": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Ask server to send email with a link to unlock an account password.",
                "tags": [
                    "unlock"
                ],
                "summary": "Ask server to send email with a link to unlock an account.",
                "parameters": [
                    {
                        "description": "User (only username is required)",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/structures.UserAsk"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK - Check your mailbox"
                    },
                    "400": {
                        "description": "Missing username in payload"
                    },
                    "500": {
                        "description": "An error occured."
                    }
                }
            }
        },
        "/change_password": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Change a user's password.",
                "tags": [
                    "change_password"
                ],
                "summary": "Change a user's password.",
                "parameters": [
                    {
                        "description": "User data",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/structures.UserChangePassword"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK - Mail changed"
                    },
                    "400": {
                        "description": "Invalid payload"
                    },
                    "500": {
                        "description": "An error occured."
                    }
                }
            }
        },
        "/reinitialize": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Reinitialize a user's password.",
                "tags": [
                    "reinitialization"
                ],
                "summary": "Reinitialize a user's password.",
                "parameters": [
                    {
                        "description": "User",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/structures.UserReinitialize"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK - Password changed"
                    },
                    "400": {
                        "description": "Missing data in payload"
                    },
                    "500": {
                        "description": "An error occured."
                    }
                }
            }
        },
        "/unlock": {
            "post": {
                "security": [
                    {
                        "BasicAuth": []
                    }
                ],
                "description": "Unlock a user.",
                "tags": [
                    "unlock"
                ],
                "summary": "Unlock a user.",
                "parameters": [
                    {
                        "description": "User",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/structures.UserUnlock"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK - User unlocked"
                    },
                    "400": {
                        "description": "Missing data in payload"
                    },
                    "500": {
                        "description": "An error occured."
                    }
                }
            }
        }
    },
    "definitions": {
        "structures.UserAsk": {
            "type": "object",
            "properties": {
                "username": {
                    "type": "string"
                }
            }
        },
        "structures.UserChangePassword": {
            "type": "object",
            "properties": {
                "new_password": {
                    "type": "string"
                },
                "old_password": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "structures.UserReinitialize": {
            "type": "object",
            "properties": {
                "new_password": {
                    "type": "string"
                },
                "token": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "structures.UserUnlock": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        }
    }
}